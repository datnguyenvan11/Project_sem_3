<section>
    <div class="banner">

    </div>

</section>


<div class="container1" style="margin-left:10%;margin-right:10%;margin-top:7% ;margin-bottom:7%">
    <div class="hd-info-pro">
        PLEASE ENTER iNFORMATION
    </div>

    <form action="/HomeInsurance/CreateContract" method="get" id="formhouse">
        <div class="row">
            <div class="col-lg-9 col-md-8 col-sm-12">
                <div class="box-border-pro">
                    <div class="frm frmpro">

                        <div class="form-group">
                            <div for="rdbPackage_0" class="row col-form-label col-form-label-first">Select InsurancePackages :</div>

                            <div class="row">

                                <div _ngcontent-c5="" class="row">
                                    @foreach (var item in Model)
                                    {
                                        <div _ngcontent-c5="" class="col-md-6" style="margin-top:30px">
                                            <div _ngcontent-c5="" class="benefit-item benefit-item-description-table">
                                                <div _ngcontent-c5="" class="benefit-item-main">
                                                    <div _ngcontent-c5="" class="benefit-icon">
                                                        <i _ngcontent-c5="" class="icon icon-benefit icon-benefit-shield"></i>
                                                    </div>
                                                    <div _ngcontent-c5="" class="benefit-image full-cover" style="background-image: url('https://ngocdiep.vn/uploads/images/img-5787-rs(1).jpg')">
                                                        <div _ngcontent-c5="" class="benefit-name-wrapper v-middle">
                                                            <h2 _ngcontent-c5="" class="benefit-name">
                                                                <div _ngcontent-c5="" class="app-checkbox agree-with-term">
                                                                    <label for="muhRadio4"><input type="radio" class="@item.Price" id='rdbPackage_1' value='@item.Id' onclick="closeForm();" name="insurancePackageId" required /></label>                                                           @*<input _ngcontent-c5="" id="choose1" name="choose1" type="checkbox"> <label _ngcontent-c5="" for="choose1">*@
                                                                    <span _ngcontent-c5="" class="checkout-term">@item.Name</span>

                                                                </div>
                                                            </h2>
                                                        </div>
